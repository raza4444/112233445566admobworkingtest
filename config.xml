<?xml version='1.0' encoding='utf-8'?>
<widget id="example.admob.basic" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>AdmobBasicExample</name>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="UseSwiftLanguageVersion" value="4" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1.3.3" />
    <engine name="ios" spec="4.5.5" />
    <engine name="android" spec="7.1.4" />
    <engine name="browser" spec="5.0.4" />
    <plugin name="cordova-admob-plus" spec="^0.24.1">
        <variable name="APP_ID_ANDROID" value="ca-app-pub-3940256099942544~3347511713" />
        <variable name="PLAY_SERVICES_VERSION" value="17.1.2" />
        <variable name="APP_ID_IOS" value="test" />
    </plugin>
    <preference name="phonegap-version" value="cli-8.0.0" />
</widget>
