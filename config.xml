<?xml version='1.0' encoding='utf-8'?>
<widget id="example.admob.basic" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Umer App Test</name>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="UseSwiftLanguageVersion" value="4" />
    </platform>
    <preference name="android-targetSdkVersion" value="26" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="onesignal" value="744dcc96-674d-4066-b170-cf66d893ed46" />
    <preference name="admobBannerId" value="ca-app-pub-2607313420010336/8685143803" />
    <preference name="admobInterstitialId" value="ca-app-pub-2607313420010336/1009053512" />
    <preference name="admobAppId" value="ca-app-pub-2607313420010336~1656701025" />
    <preference name="SplashScreenDelay" value="4000" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="FadeSplashScreenDuration" value="400" />
    <preference name="SplashScreenBackgroundColor" value="white" />
    <preference name="phonegap-version" value="cli-8.0.0" />
    <engine name="ios" spec="4.5.5" />
    <engine name="android" spec="7.1.4" />
    <engine name="browser" spec="5.0.4" />
    <plugin name="cordova-plugin-whitelist" spec="1.3.3" />
    <plugin name="cordova-admob-plus" spec="^0.24.1">
        <variable name="APP_ID_ANDROID" value="ca-app-pub-3940256099942544~3347511713" />
        <variable name="PLAY_SERVICES_VERSION" value="17.1.2" />
        <variable name="APP_ID_IOS" value="test" />
    </plugin>
    <plugin name="onesignal-cordova-plugin" spec="2.4.6" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="cordova-plugin-statusbar" spec="2.4.2" />
    <plugin name="cordova-plugin-network-information" spec="2.0.1" />
    <plugin name="cordova-plugin-customconfigparameters" spec="^3.0.0" />
    <plugin name="cordova-plugin-appversion" spec="^1.0.0" />
    <plugin name="com.bunkerpalace.cordova.YoutubeVideoPlayer" spec="https://github.com/JonSmart/CordovaYoutubeVideoPlayer" />
    
</widget>
